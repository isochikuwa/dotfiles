  "python.venvPath": "~/.cache/pypoetry/virtualenvs",
  "rust-analyzer.procMacro.enable": true,
  "rust-analyzer.lens.debug.enable": false,
  "rust-analyzer.lens.run.enable": false,
  "languageserver": {
    "elmLS": {
      "command": "elm-language-server",
